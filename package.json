{
  "name": "node-mt5-connector",
  "version": "1.0.0",
  "description": "Enterprise-Grade MT5 Connector SDK for Node.js with ZeroMQ messaging, advanced security, and comprehensive trading APIs",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance",
    "test:security": "jest --testPathPattern=security",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "docs:serve": "typedoc src/index.ts --watch",
    "security:audit": "npm audit",
    "security:snyk": "snyk test",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && npm run test",
    "release": "semantic-release",
    "clean": "rimraf dist coverage docs"
  },
  "keywords": [
    "mt5",
    "metatrader",
    "trading",
    "forex",
    "zeromq",
    "financial",
    "connector",
    "sdk",
    "typescript",
    "enterprise"
  ],
  "author": {
    "name": "MT5 Connector Team",
    "email": "support@mt5connector.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mt5-connector/node-mt5-connector.git"
  },
  "bugs": {
    "url": "https://github.com/mt5-connector/node-mt5-connector/issues"
  },
  "homepage": "https://github.com/mt5-connector/node-mt5-connector#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "zeromq": "^6.0.0",
    "libsodium-wrappers": "^0.7.11",
    "winston": "^3.8.2",
    "joi": "^17.9.2",
    "eventemitter3": "^5.0.1",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prom-client": "^14.2.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@types/jest": "^29.5.3",
    "@types/uuid": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "prettier": "^3.0.0",
    "rollup": "^3.26.3",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.24.8",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "snyk": "^1.1193.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.ts"]
  }
}